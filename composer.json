  "require": {
    "php": "^7.1",
    "php-http/httplug": "^1.0",
    "php-http/message": "^1.5",
    "symfony/serializer": "^3.2",
    "symfony/property-access": "^3.2"
  },
  "require-dev": {
    "php-http/guzzle6-adapter": "^1.0",
    "squizlabs/php_codesniffer": "^3.0",
