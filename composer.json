  "type": "package",
  "license": "MIT",
  "keywords": ["redmine", "teacat", "api"],
  "authors": [
    {
      "name": "Martin Kuzma",
      "email": "martin.kuzma90@gmail.com"
    },
    {
      "name": "Jakub Janata",
      "email": "jakubjanata@gmail.com"
    }
